<% include headmeta %>

<!-- BEGIN BODY -->
<body class="page-header-fixed page-full-width page-header-fixed-mobile page-footer-fixed">
<% include header %>

<!-- BEGIN CONTAINER -->
<div class="page-container">
    <!-- BEGIN PAGE -->
    <div class="page-content-wrapper">
        <div class="page-content">

            <!-- BEGIN PAGE HEADER-->
            <div class="row">
                <div class="col-md-8 col-lg-8">
                    <h3 class="page-title">
                        实时成本核算 <small>每日用能&amp;产出</small>
                    </h3>
                </div>
                <div class="col-md-4 col-lg-4">
                    <h3 class="page-title pull-right">2014年7月15日 14:20:18</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 col-lg-12">
                    <ul class="page-breadcrumb breadcrumb" style="margin-bottom: 10px;">
                        <li>
                            <i class="fa fa-home"></i>
                            <a href="#">今日能源资源消耗</a>
                        </li>
                    </ul>
                </div>
            </div>
            <!-- END PAGE HEADER-->

            <!-- BEGIN DASHBOARD STATS -->
            <div class="row">
                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                    <div class="dashboard-stat blue">
                        <div class="visual">
                            <i class="fa fa-tint"></i>
                        </div>
                        <div class="title">
                            本日用水
                        </div>
                        <div class="details">
                            <div class="number">
                                1349
                            </div>
                            <div class="desc">
                                吨
                            </div>
                        </div>
                        <a class="more" href="#"></a>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                    <div class="dashboard-stat green">
                        <div class="visual">
                            <i class="fa fa-bolt"></i>
                        </div>
                        <div class="title">
                            本日用电
                        </div>
                        <div class="details">
                            <div class="number">549</div>
                            <div class="desc">度</div>
                        </div>
                        <a class="more" href="#">
                        </a>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                    <div class="dashboard-stat purple">
                        <div class="visual">
                            <i class="fa fa-fire"></i>
                        </div>
                        <div class="title">
                            本日用气
                        </div>
                        <div class="details">
                            <div class="number">89</div>
                            <div class="desc">立方</div>
                        </div>
                        <a class="more" href="#">
                        </a>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                    <div class="dashboard-stat yellow">
                        <div class="visual">
                            <i class="fa fa-folder-o"></i>
                        </div>
                        <div class="title">
                            本日用米
                        </div>
                        <div class="details">
                            <div class="number">12</div>
                            <div class="desc">袋</div>
                        </div>
                        <a class="more" href="#"></a>
                    </div>
                </div>
            </div>
            <!-- END DASHBOARD STATS -->

            <div class="row">
                <div class="col-md-3 col-lg-3">
